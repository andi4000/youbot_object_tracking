<launch>
	<param name="/youbotPID/linear_y/Kp" value="0.7" type="double" />
	<param name="/youbotPID/linear_y/Ki" value="0" type="double" />
	<param name="/youbotPID/linear_y/Kd" value="0.09" type="double" />
	<param name="/youbotPID/linear_y/iLimitHi" value="0.7" type="double" />
	<param name="/youbotPID/linear_y/iLimitLo" value="-0.7" type="double" />
	<param name="/youbotPID/linear_y/speed" value="0.4" type="double" />
	
	<param name="/youbotPID/angular_z/Kp" value="0.5" type="double" />
	<param name="/youbotPID/angular_z/Ki" value="0" type="double" />
	<param name="/youbotPID/angular_z/Kd" value="0" type="double" />
	<param name="/youbotPID/angular_z/iLimitHi" value="0.7" type="double" />
	<param name="/youbotPID/angular_z/iLimitLo" value="-0.7" type="double" />
	<param name="/youbotPID/angular_z/speed" value="0.3" type="double" />

	<node name="colorblobtracking" pkg="youbot_object_tracking" type="colorblobtracking" />
	<node name="makeitmove" pkg="youbot_object_tracking" type="makeitmove" output="screen" />
</launch>
